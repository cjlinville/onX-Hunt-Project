{
  "hunting_district.geojson": {
    "source": "Montana FWP",
    "type": "polygon",
    "field_map": {
      "HD_ID": "unit_id",
      "SPECIES": "species",
      "NAME": "unit_name"
    },
    "value_maps": {}
  },
  "public_lands.geojson": {
    "source": "Montana FWP Public Lands",
    "type": "polygon",
    "field_map": {
      "SITETYPE": "owner_type",
      "SITENAME": "agency",
      "PUBLIC_ACCESS": "public_access"
    },
    "value_maps": {}
  },
  "parcels.geojson": {
    "source": "Montana Parcels",
    "type": "polygon",
    "field_map": {
      "OwnerName": "Owner",
      "TotalValue": "Total_Value"
    },
    "value_maps": {}
  },
  "mt_roads.geojson": {
    "source": "Montana MSDI Transportation",
    "type": "network",
    "field_map": {
      "LSt_Name": "Name",
      "ROADCLASS": "Road_Class",
      "SURFACE": "surface",
      "ACCESS": "motorized_access"
    },
    "value_maps": {
      "motorized_access": {
        "Y": "yes",
        "N": "no",
        "OPEN": "yes",
        "CLOSED": "no"
      }
    }
  },
  "fs_trails.geojson": {
    "source": "USFS Trails",
