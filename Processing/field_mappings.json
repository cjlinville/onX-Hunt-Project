    "field_map": {
      "TRAIL_NAME": "Name",
      "TRAIL_TYPE": "Trail_Type",
      "TERRA_MOTORIZED": "Motorized Use",
      "SNOW_MOTORIZED": "Snowmobile Use"
    },
    "value_maps": {}
  },
  "nhd_flowline.geojson": {
    "source": "USGS NHD",
    "type": "network",
    "field_map": {
      "GNIS_NAME": "Name"
    },
    "value_maps": {}
  },
  "nhd_waterbody.geojson": {
    "source": "USGS NHD",
    "type": "polygon",
    "field_map": {
      "GNIS_NAME": "Name"
    },
    "value_maps": {}
  },
  "nhd_area.geojson": {
    "source": "USGS NHD",
    "type": "polygon",
    "field_map": {
      "GNIS_NAME": "Name",
      "FTYPE": "Water_Type"
    },
    "value_maps": {}
